    "interactjs": "^1.10.0",
    "jingle": "^3.0.3",
    "jingle-session": "^2.0.3",
    "jquery": "^3.5.1",
    "jxt": "^4.0.2",
    "jxt-xmpp": "^3.3.0",
    "jxt-xmpp-types": "^3.0.0",
    "mini-css-extract-plugin": "^1.3.1",
    "moment": "^2.29.1",
    "otr": "https://github.com/sualko/otr.git#1ee459b4eda85d0a7d76d7da7b3a36861294346e",
    "precompile-handlebars": "^1.0.5",
    "prettier": "^2.2.1",
    "qrcode": "^1.4.4",
    "sass": "^1.32.8",
    "screenfull": "^5.0.2",
    "sha1": "^1.1.1",
